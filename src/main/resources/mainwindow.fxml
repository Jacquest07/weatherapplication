    <ImageView fx:id="image" fitHeight="80.0" fitWidth="104.0">
        <image>
            <Image url="Kafelek-witamina-D-slonce.jpg" />
        </image>
    </ImageView>
    <Label fx:id="temperature" prefHeight="22.0" prefWidth="155.0" text="Srednia temperatura to: " />
    <Label fx:id="temperatureF" text="Srednia temperatura w Farenheit to: " />
</VBox>
